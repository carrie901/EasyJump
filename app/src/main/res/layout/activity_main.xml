<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.lovejjfg.easyjump.MainActivity">

        <Button
            android:id="@+id/bt_show_web"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="打开浏览器"
            />


        <Button
            android:id="@+id/bt_t1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="通知跳转Test1"
            />

        <Button
            android:id="@+id/bt_t2"
            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="通知跳转Test2"
            />

        <Button
            android:id="@+id/bt_web"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="通知跳转Web"
            />


        <Button
            android:id="@+id/bt_t11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="跳转Test1"
            />

        <Button
            android:id="@+id/bt_t22"
            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="跳转Test2"
            />

        <Button
            android:id="@+id/bt_web1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="跳转Web"
            />


    </LinearLayout>

</android.support.v4.widget.NestedScrollView>

